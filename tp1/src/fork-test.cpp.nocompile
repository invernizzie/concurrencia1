#include <iostream>
#include "fork.h"
using namespace std;

class Saludador : public Proceso {
    public:
        Saludador() {};
        virtual void ejecutar();
        virtual ~Saludador() {};
};

void Saludador :: ejecutar() {
    cout << "Hola :)" << endl;
}

int main() {
    Saludador saludador;
    try {
        fork(saludador);
    }
    catch (int errno) {
        // TODO: Print error
        exit(1);
    }
    
    cout << "Yo sigo vivo" << endl;
    return 0;
}
